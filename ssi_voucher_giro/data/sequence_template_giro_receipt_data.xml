<odoo>
<data noupdate="1">
    <record id="sequence_template_giro_receipt" model="sequence.template">
        <field name="name">Standard: Giro Receipt</field>
    <field name="model_id" ref="ssi_voucher_giro.model_account_giro_receipt" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.giro_receipt'),('name','=','name')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','account.giro_receipt'),('name','=','date_voucher')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_giro_receipt" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>
</odoo>
