<openerp>
<data>

<record id="account_cheque_voucher_view_tree" model="ir.ui.view">
    <field name="name">account.cheque_voucher tree</field>
    <field name="model">account.cheque_voucher</field>
    <field name="inherit_id" ref="account_voucher_bank_cash.account_voucher_bank_cash_common_view_tree"/>
    <field name="mode">primary</field>
    <field name="priority" eval="70"/>
    <field name="arch" type="xml">
        <data>
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="payee_partner_id"/>
                <field name="source_bank_id"/>
            </xpath>
            <xpath expr="//field[@name='date_voucher']" position="after">
                <field name="date_issue"/>
            </xpath>
        </data>

    </field>
</record>

<record id="account_cheque_voucher_view_form" model="ir.ui.view">
    <field name="name">account.cheque_voucher form</field>
    <field name="model">account.cheque_voucher</field>
    <field name="inherit_id" ref="account_voucher_bank_cash.account_voucher_bank_cash_common_view_form"/>
    <field name="mode">primary</field>
    <field name="priority" eval="70"/>
    <field name="arch" type="xml">
        <data>
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="payee_partner_id" domain="[('is_company','=',False)]"/>
                <field name="source_bank_id"/>
            </xpath>
            <xpath expr="//field[@name='date_voucher']" position="after">
                <field name="date_issue"/>
            </xpath>
        </data>
    </field>
</record>

</data>
</openerp>
