<data noupdate="1">
<record id="sequence_giro_payment" model="ir.sequence">
    <field name="name">General Sequence: Giro Payment</field>
    <field name="padding">6</field>
    <field name="prefix">GIRO-PAY/%(y)s/</field>
    <field eval="1" name="number_increment" />
</record>
<record id="sequence_giro_receipt" model="ir.sequence">
    <field name="name">General Sequence: Giro Receipt</field>
    <field name="padding">6</field>
    <field name="prefix">GIRO-RCPT/%(y)s/</field>
    <field eval="1" name="number_increment" />
</record>
</data>
