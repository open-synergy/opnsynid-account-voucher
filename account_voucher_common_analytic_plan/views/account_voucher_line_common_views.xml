<openerp>
    <data>

        <record id="account_voucher_line_common_view_tree" model="ir.ui.view">
            <field name="name">Inherit :Tree Account Voucher Line Common</field>
            <field name="model">account.voucher_line_common</field>
            <field name="inherit_id" ref="account_voucher_common.account_voucher_line_common_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="replace">
                    <field name="analytic_plan_account_id"/>
                </xpath>
            </field>
        </record>

        <record id="account_voucher_line_common_view_form" model="ir.ui.view">
            <field name="name">Inherit :Form Account Voucher Line Common</field>
            <field name="model">account.voucher_line_common</field>
            <field name="inherit_id" ref="account_voucher_common.account_voucher_line_common_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="replace">
                    <field name="analytic_plan_account_id"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
