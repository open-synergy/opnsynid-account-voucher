<odoo>
    <data noupdate="1">
        <record id="sequence_invoice_settlement" model="ir.sequence">
            <field name="name">General Sequence: Invoice Settlement</field>
            <field name="code">account.invoice_settlement</field>
            <field name="prefix">INV-STLMT/%(y)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>
    </data>
</odoo>
