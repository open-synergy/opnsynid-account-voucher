<odoo>
    <record id="voucher_type_bank_receipt" model="account.voucher_type">
        <field name="name">Bank Receipt</field>
        <field name="code">BNK-RCPT</field>
        <field name="header_type">dr</field>
        <field name="create_header_item" eval="1" />
