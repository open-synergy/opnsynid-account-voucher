    <record id="voucher_type_cash_receipt" model="account.voucher_type">
        <field name="name">Cash Receipt</field>
        <field name="code">CASH-RCPT</field>
        <field name="header_type">dr</field>
        <field name="create_header_item" eval="1" />
        <field name="check_total" eval="1" />
        <field name="check_debit_credit" eval="0" />
        <field name="detail_allow_negative" eval="1" />
    </record>

    <record id="voucher_type_cash_payment" model="account.voucher_type">
