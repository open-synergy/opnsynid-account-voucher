<openerp>
    <data>
        <record id="account_cheque_receipt_view_form" model="ir.ui.view">
            <field name="name">account.cheque_receipt form</field>
            <field name="model">account.cheque_receipt</field>
            <field
                name="inherit_id"
                ref="account_voucher_cheque.account_cheque_receipt_view_form"
            />
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <data>
                    <xpath
                        expr="//field[@name='line_cr_ids']/form//field[@name='account_id']"
                        position="after"
                    >
                        <field name="date_start" />
                        <field name="date_end" />
                    </xpath>
                    <xpath
                        expr="//field[@name='line_cr_ids']/tree/field[@name='account_id']"
                        position="after"
                    >
                        <field name="date_start" />
                        <field name="date_end" />
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</openerp>
