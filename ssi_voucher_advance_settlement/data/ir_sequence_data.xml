<odoo>
<data noupdate="1">
<record id="sequence_purchase_advance_settlement" model="ir.sequence">
    <field name="name">General Sequence: Purchase Advance Settlement</field>
    <field name="code">account.purchase_advance_settlement</field>
    <field name="prefix">PURC-ADV-STLMT/%(y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
<record id="sequence_sale_advance_settlement" model="ir.sequence">
    <field name="name">General Sequence: Sale Advance Settlement</field>
    <field name="code">account.sale_advance_settlement</field>
    <field name="prefix">SALE-ADV-STLMT/%(y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
