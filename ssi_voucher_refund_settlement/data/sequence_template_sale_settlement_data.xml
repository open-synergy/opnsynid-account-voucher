<odoo>
<data noupdate="1">
<record id="sequence_template_sale_refund_settlement" model="sequence.template">
    <field name="name">Standard: Sale Refund Settlement</field>
    <field
                name="model_id"
                ref="ssi_voucher_refund_settlement.model_account_sale_refund_settlement"
            />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','account.sale_refund_settlement'),('name','=','name')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','account.sale_refund_settlement'),('name','=','date_voucher')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_sale_refund_settlement" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>
</odoo>
