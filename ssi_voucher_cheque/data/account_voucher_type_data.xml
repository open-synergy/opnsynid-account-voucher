<odoo>
    <record id="voucher_type_cheque_receipt" model="account.voucher_type">
        <field name="name">Cheque Receipt</field>
        <field name="code">CHQ-RCPT</field>
        <field name="header_type">dr</field>
        <field name="create_header_item" eval="1" />
        <field name="check_total" eval="1" />
        <field name="check_debit_credit" eval="0" />
        <field name="detail_allow_negative" eval="1" />
    </record>

    <record id="voucher_type_cheque_payment" model="account.voucher_type">
        <field name="name">Cheque Payment</field>
        <field name="code">CHQ-PAY</field>
        <field name="header_type">cr</field>
        <field name="create_header_item" eval="1" />
        <field name="check_total" eval="1" />
        <field name="check_debit_credit" eval="0" />
        <field name="detail_allow_negative" eval="1" />
    </record>
</odoo>
